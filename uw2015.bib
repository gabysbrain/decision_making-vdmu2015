  number	= {6},
  pages		= {1458--1467},
  pmid		= {20975187},
  rating	= {4},
  read		= {Yes},
  title		= {World Lines},
  url		= {http://www.ncbi.nlm.nih.gov/pubmed/20975187},
  volume	= {16},
